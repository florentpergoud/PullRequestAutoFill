{
  "name": "PullRequestAutoFill",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://github.com/bamlab/ze-turf/pull/*","https://github.com/bamlab/ze-turf/compare/*"],
      "run_at": "document_idle",
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
